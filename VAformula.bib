keywords = {Buildings,Computer Graphics,Computer Simulation,Computer Society,Computer-Assisted,Data Display,Data Interpretation,Data analysis,Data visualization,Databases,Factual,Frequency,Image Interpretation,Information Storage and Retrieval,Information visualization,Models,Parallel Sets,Performance analysis,Statistical,Statistics,Switches,User-Computer Interface,categorical data,categorical data interactive exploration,categorical data visualization,data analysis,data visualisation,housing data,information visualization,interaction,interactive systems,large CRM data set,multivariate data.,nominal data,very large databases},
